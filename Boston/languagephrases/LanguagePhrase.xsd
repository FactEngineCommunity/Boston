<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" xmlns:mstns="http://www.viev.com" xmlns="http://www.viev.com" attributeFormDefault="qualified" elementFormDefault="qualified" targetNamespace="http://www.viev.com" id="LanguageParserRules" xmlns:xs="http://www.w3.org/2001/XMLSchema">
