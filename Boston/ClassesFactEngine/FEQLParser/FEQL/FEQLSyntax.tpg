[Color(94, 213, 165)] BRCLOSE 	-> @"\)+";
[Color(94, 213, 165)]  COLON		-> ":";
COLUMNNAMESTR -> @"[aA-zZ0-9]+";
[Color(135, 207, 243)]	COMMA         -> ",";
CURLYBRACKETCLOSE -> @"\}+";
CURLYBRACKETOPEN -> @"\{+";
DOUBLEQUOTE   -> @"""";
EOF           -> @"^$";
EQUALS        -> "=";
FACTTYPENAME -> @"[aA-zZ]+";
[Color(216, 127, 178)]	IDENTIFIER -> @"[aA-zZ0-9 \-]+";
MULTDIV 	-> @"\*|/";
MODELNAME     -> @"[aA-zZ]+";
[Color(94, 213, 165)]	MODELELEMENTNAME -> @"(([A-Z]+[a-z]+)+[\s]?)+";//@"[aA-zZ]+";
[Color(94, 213, 165)]	MODELELEMENTSUFFIX ->@"([0-9])+";
MODELID	-> @"[aA-zZ0-9\-]+";
NUMBER 	-> @"[0-9]+";
