[Color(216, 127, 178)]	SINGLEQUOTE   -> @"'";
STAR          -> "\*";
[Color(0, 0, 255)]	SPACE		-> "\s";
USERTABLENAME -> @"[aA-zZ0-9]+";
WHERECLAUSECOLUMNNAMESTR -> @"[aA-zZ0-9]+";
[Color(153, 0, 0)]	VALUE         -> @"[aA-zZ0-9\-\s+\#\*\?]*";

